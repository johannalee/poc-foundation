{
  "name": "sch",
  "version": "0.0.0",
  "description": "Schematics for POC lib project",
  "keywords": [
    "angular",
    "schematics",
    "components",
    "component library",
    "structure",
    "folder structure",
    "best practices"
  ],
  "scripts": {
    "build": "npm run build:schematics && npm run copy:schematics",
    "build:schematics": "../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schematics": "cpr . ../dist/sch --overwrite --deleteFirst"
  },
  "author": "Johanna Lee",
  "license": "MIT",
  "schematics": "./schemacits/collection.json",
  "peerDependencies": {
    "@angular-devkit/core": "^7.3.9",
    "@angular-devkit/schematics": "^7.3.9",
    "cpr": "^3.0.1"
  }
}
