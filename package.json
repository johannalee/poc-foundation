{
  "name": "poc-foundation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build lib",
    "build:storybook": "build-storybook",
    "npm:pack": "cd dist/lib && npm pack",
    "package": "npm run build:lib && npm run npm:pack",
    "storybook": "start-storybook -p 6006",
    "test:gen-output": "ng test --json --outputFile=test-results.json --watch",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --config .prettierrc --list-different \"./lib/**/*.{ts,js,scss}\"",
    "format:fix": "prettier --config .prettierrc --write \"./lib/**/*.{ts,js,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng test && npm run format"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/jest": "^24.0.12",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-jest": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/angular": "^5.0.11",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/node": "~8.9.4",
    "@types/puppeteer": "^1.12.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "codelyzer": "~4.5.0",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "jest-image-snapshot": "^2.8.1",
    "jest-preset-angular": "^7.1.0",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.15.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
